Reals R

Real a1, a2
Interval A
A := CreateInterval(a1, a2)
Subset(A, R)

Real x
In(x, A)

Function f
f := CreateFunction(A, R)

Real y
y := apply(f, x)

Point p
p := Pt(x, y)

Real dfx 
dfx := derivativeAtP(f, x)

Real ifA
ifA := integral(A, f)

AutoLabel All
Label R $\mathbb{R}$
Label y $f(x)$
Label dfx $f'(x)$
Label ifA $\int_{A} f(x) dx$
NoLabel a1, a2